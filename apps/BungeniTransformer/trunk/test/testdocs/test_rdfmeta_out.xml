            <bungeni:bungenimeta>#id414870631               
               <bungeni:BungeniPersonID>212</bungeni:BungeniPersonID>               
               <bungeni:BungeniPersonRole>212</bungeni:BungeniPersonRole>               
               <bungeni:BungeniSectionID>o4btCkyETC2KsQJwdeLu6w</bungeni:BungeniSectionID>               
               <bungeni:BungeniSectionType>Speech</bungeni:BungeniSectionType>               
