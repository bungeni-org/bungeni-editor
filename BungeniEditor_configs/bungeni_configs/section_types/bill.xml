<?xml version="1.0" encoding="windows-1252"?>
<sectionTypes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
 for="bill" xmlns:an="#an" >
 
 <!-- root is mandatory, all mode="call" attributed calls need sectionType definitions -->
 
 <sectionType name="root"
		prefix="bill"
		numstyle="single"
		visibility="user" >
		<output>
		<content>
			<an:akomaNtoso>
				<an:bill>
					<an:meta mode="call"/>
					<an:preface mode="call" />
					<an:preamble mode="call" />
					<an:body  mode="call"/>
					<an:conclusions mode="call" />
				</an:bill>
			</an:akomaNtoso>
		</content>
	   </output>
	 </sectionType>
	 
   <sectionType name="meta"
        prefix="meta"
        numstyle="single">
        <output>
            <content>
                <an:meta />
            </content>
        </output>
    </sectionType>
	 
    <sectionType name="Preamble" 
        prefix="preamble" 
        numstyle="single" 
        >
        <output>
            <content>
                <an:preamble/>
            </content>
        </output>
    </sectionType>

    <sectionType name="Preface" 
        prefix="preface" 
        numstyle="single" 
         >
        <output>
            <content>
                <an:preface/>
            </content>
        </output>
    </sectionType>
    
	<sectionType name="BillBody"
		 prefix="billbody"
		 numstyle="single"
		 background="0xf0f0f0"
		 indent-left="0"
		 indent-right="0"
		 visibility="user"
		 >
     <output>
         <content>
             <an:body />
         </content>
     </output>
    </sectionType>

	<sectionType name="Conclusion"
     prefix="conclusion"
     numstyle="single"
     background="0xffdfdf"
     indent-left="0"
     indent-right="0"
     visibility="user">
     <output>
         <content>
             <an:conclusions />
         </content>
     </output>
  </sectionType>	
	
    <sectionType name="Article" prefix="article" numstyle="serial" background="0xf0f0f0"
        indent-left=".3" indent-right="0" visibility="user">
        <numbering scheme="NUMERIC" decorator="flowerBracket"/>
        <output>
            <content>
                <an:article />
            </content>
        </output>
    </sectionType>
    
    <sectionType name="Chapter" prefix="chapter" numstyle="serial" background="0xe8ee88"
        indent-left=".4" indent-right="0" visibility="user">
        <numbering scheme="ROMAN-Upper" decorator="none"/>
        <output>
            <content>
                <an:chapter />
            </content>
        </output>
    </sectionType>
    
    <sectionType name="Clause" prefix="clause" numstyle="serial" background="0xffdfdf"
        indent-left=".5" indent-right="0" visibility="user">
        <numbering scheme="ROMAN" decorator="hashPrefix"/>
        <output>
            <content>
                <an:clause />
            </content>
        </output>
    </sectionType>
    
    <sectionType name="LongTitle" prefix="longtitle" numstyle="single" background="0xffffff"
        indent-left=".3" indent-right="0" visibility="user"> 
        <output>
            <content>
                <an:longTitle />
            </content>
        </output>
    </sectionType>
    
    <sectionType name="Paragraph" prefix="para" numstyle="serial" background="0xffff00"
        indent-left=".7" indent-right="0" visibility="user">
        <numbering scheme="ALPHABETICAL-Lower" decorator="parens"/>
        <output>
            <content>
                <an:paragraph />
            </content>
        </output>
    </sectionType>

    <sectionType name="Part" prefix="part" numstyle="serial" background="0xffffa1" indent-left=".3"
        indent-right="0" visibility="user">
        <numbering scheme="ALPHABETICAL" decorator="postDashed"/>
        <output>
            <content>
                <an:part/>
            </content>
        </output>
    </sectionType>

    <sectionType name="Section" prefix="section" numstyle="serial" background="0xffaa00"
        indent-left=".6" indent-right="0" visibility="user">
        <numbering scheme="ALPHABETICAL-Lower" decorator="parens"/>
        <output>
            <content>
                <an:section/>
            </content>
        </output>
    </sectionType>
 
    <sectionType name="ShortTitle" prefix="shorttitle" numstyle="single" background="0xffffff"
        indent-left=".2" indent-right="0" visibility="user">
        <!-- this should perhaps be an inlinetype -->
        <output>
            <content>
                <an:shortTitle/>
            </content>
        </output>

    </sectionType>
    
    <sectionType name="SubChapter" prefix="subchapter" numstyle="serial" background="0xffaa00"
        indent-left=".5" indent-right="0" visibility="user">
        <numbering scheme="ROMAN-Upper" decorator="postDotted"/>
        <output>
            <content>
                <an:subchapter/>
            </content>
        </output>
    </sectionType>
    
    <sectionType name="SubClause" prefix="subclause" numstyle="serial" background="0xffdfdf"
        indent-left=".8" indent-right="0" visibility="user">
        <numbering scheme="ALPHABETICAL-Lower" decorator="hashPrefix"/>
        <output>
            <content>
                <an:subclause/>
            </content>
        </output>
    </sectionType>
    
    <sectionType name="SubParagraph" prefix="subpara" numstyle="serial" background="0xffff00"
        indent-left=".8" indent-right="0" visibility="user">
        <numbering scheme="ROMAN-Lower" decorator="parens"/>
        <output>
            <content>
                <an:subparagraph/>
            </content>
        </output>
    </sectionType>
    
    <sectionType name="SubSection" prefix="subsection" numstyle="serial" background="0x00ffff"
        indent-left=".8" indent-right="0" visibility="user">
        <numbering scheme="ROMAN-Upper" decorator="square"/>
        <output>
            <content>
                <an:subsection/>
            </content>
        </output>
    </sectionType>
    
    <sectionType name="Title" prefix="title" numstyle="single" background="0xffffff"
        indent-left=".1" indent-right="0" visibility="user">
        <output>
            <content>
                <an:title/>
            </content>
        </output>
    </sectionType>
	

    
</sectionTypes>
