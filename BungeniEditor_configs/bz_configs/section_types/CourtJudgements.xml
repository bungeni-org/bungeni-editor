<?xml version="1.0" encoding="windows-1252"?>
<sectionTypes for="CourtJudgements" xmlns:an="#an">
 
    <!-- root is mandatory, all mode="call" attributed calls need sectionType definitions -->
 
    <sectionType name="root"
              prefix="CourtJudgements"
              numstyle="single"
              visibility="user" >
        <output>
            <content>
                <an:akomaNtoso>
                    <an:judgement>
                        <an:meta mode="call"/>
                        <an:heading mode="call" />
                        <an:judgementBody  mode="call"/>
                        <an:dec-statement mode="call"/>
                        <an:conclusions mode="call" />
                    </an:judgement>
                </an:akomaNtoso>
            </content>
        </output>
    </sectionType>
	 
