<?xml version="1.0" encoding="windows-1252"?>
<sectionTypes xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
              xsi:noNamespaceSchemaLocation="sectionTypes.xsd"
              for="act">
 
    <!-- root is mandatory, all mode="call" attributed calls need sectionType definitions -->
 
    <sectionType name="root"
              prefix="act"
              numstyle="single"
              visibility="user" >
        <output>
            <content>
                <akomaNtoso>
                    <act>
                        <meta mode="call"/>
                        <preface mode="call" />
                        <preamble mode="call" />
                        <body  mode="call"/>
                        <conclusions mode="call" />
                    </act>
                </akomaNtoso>
            </content>
        </output>
    </sectionType>
	 
    <sectionType name="meta"
                prefix="meta"
                numstyle="single">
        <output>
            <content>
                <meta/>
               
            </content>
        </output>
    </sectionType>
	 
   
    <sectionType name="Preface" 
                 prefix="preface" 
                 numstyle="single" 
                 background="0xa1a1a1"
    >
        <output>
            <content>
                <preface/>
            </content>
        </output>
    </sectionType>

  
    <sectionType name="Preamble" 
                 prefix="preamble" 
                 numstyle="single" 
                 background="0xa1a1a1"
    >
        <output>
            <content>
                <preamble/>
            </content>
        </output>
    </sectionType>

    <sectionType name="ActBody"
                     prefix="body"
                     numstyle="single"
                     background="0xf0f0f0"
                     indent-left="0"
                     indent-right="0"
                     visibility="user"
    >
        <output>
            <content>
                <body />
            </content>
        </output>
    </sectionType>

    <sectionType name="Conclusions"
                     prefix="conclusion"
                     numstyle="single"
                     background="0xffdfdf"
                     indent-left="0"
                     indent-right="0"
                     visibility="user">
        <output>
            <content>
                <conclusions />
            </content>
        </output>
    </sectionType>	
	      
    <sectionType name="Books" prefix="books" numstyle="serial" background="0xf0f0f0"
                      indent-left=".3" indent-right="0" visibility="user">
        <numbering scheme="NUMERIC" decorator="flowerBracket"/>
        <output>
            <content>
                <hcontainer />
            </content>
        </output>
    </sectionType>
    
    <sectionType name="Book" prefix="B" numstyle="serial" background="0xf0f0f0"
                  indent-left=".3" indent-right="0" visibility="user">
        <numbering scheme="NUMERIC" decorator="flowerBracket"/>
        <output>
            <content>
                <book />
            </content>
        </output>
    </sectionType>
    
    <sectionType name="Parts" prefix="parts" numstyle="serial" background="0xf0f0f0"
                  indent-left=".3" indent-right="0" visibility="user">
        <numbering scheme="NUMERIC" decorator="flowerBracket"/>
        <output>
            <content>
                <hcontainer />
            </content>
        </output>
    </sectionType>
    
    <sectionType name="Part" prefix="P" numstyle="serial" background="0xf0f0f0"
                  indent-left=".3" indent-right="0" visibility="user">
        <numbering scheme="NUMERIC" decorator="flowerBracket"/>
        <output>
            <content>
                <part />
            </content>
        </output>
    </sectionType>
    
    <sectionType name="Chapters" prefix="chapters" numstyle="serial" background="0xf0f0f0"
                  indent-left=".3" indent-right="0" visibility="user">
        <numbering scheme="NUMERIC" decorator="flowerBracket"/>
        <output>
            <content>
                <hcontainer />
            </content>
        </output>
    </sectionType>
    
    <sectionType name="Chapter" prefix="C" numstyle="serial" background="0xe8ee88"
                 indent-left=".4" indent-right="0" visibility="user">
        <numbering scheme="ROMAN-Upper" decorator="none"/>
        <output>
            <content>
                <chapter />
            </content>
        </output>
    </sectionType>
    
    <sectionType name="Sections" prefix="sections" numstyle="serial" background="0xf0f0f0"
                  indent-left=".3" indent-right="0" visibility="user">
        <numbering scheme="NUMERIC" decorator="flowerBracket"/>
        <output>
            <content>
                <hcontainer />
            </content>
        </output>
    </sectionType>
    
    <sectionType name="Section" prefix="S" numstyle="serial" background="0xf0f0f0"
                  indent-left=".3" indent-right="0" visibility="user">
        <numbering scheme="NUMERIC" decorator="flowerBracket"/>
        <output>
            <content>
                <section />
            </content>
        </output>
    </sectionType>
    
    <sectionType name="Articles" prefix="articles" numstyle="serial" background="0xf0f0f0"
                  indent-left=".3" indent-right="0" visibility="user">
        <numbering scheme="NUMERIC" decorator="flowerBracket"/>
        <output>
            <content>
                <hcontainer />
            </content>
        </output>
    </sectionType>
    
    <sectionType name="Article" prefix="A" numstyle="serial" background="0xf0f0f0"
                  indent-left=".3" indent-right="0" visibility="user">
        <numbering scheme="NUMERIC" decorator="flowerBracket"/>
        <output>
            <content>
                <article />
            </content>
        </output>
    </sectionType>
  
    <sectionType name="LongTitle" prefix="longtitle" numstyle="single" background="0xffffff"
                 indent-left=".3" indent-right="0" visibility="user"> 
        <output>
            <content>
                <longTitle />
            </content>
        </output>
    </sectionType>
    
      <sectionType name="Num" prefix="num" numstyle="single" background="0xffffff"
                 indent-left=".3" indent-right="0" visibility="user"> 
        <output>
            <content>
                <num />
            </content>
        </output>
    </sectionType>
    
    <sectionType name="Heading" prefix="heading" numstyle="single" background="0xffffff"
                 indent-left=".1" indent-right="0" visibility="user">
        <output>
            <content>
                <heading />
            </content>
        </output>
    </sectionType>
    
     <sectionType name="Subheading" prefix="subheading" numstyle="single" background="0xffffff"
                 indent-left=".1" indent-right="0" visibility="user">
        <output>
            <content>
                <subheading />
            </content>
        </output>
    </sectionType>
    
    <sectionType name="Content" prefix="content" numstyle="single" background="0xffffff"
                 indent-left=".1" indent-right="0" visibility="user">
        <output>
            <content>
                <content />
            </content>
        </output>
    </sectionType>
  
    
