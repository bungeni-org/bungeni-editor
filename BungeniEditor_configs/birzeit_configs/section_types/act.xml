<sectionTypes for="act" xmlns:an="http://www.akomantos.org/2.0" xmlns:muqid="http://muqtafi.birzeit.edu/proprietary" >
 
    <!-- root is mandatory, all mode="call" attributed calls need sectionType definitions -->
 
    <sectionType name="root"
                 prefix="act"
                 numstyle="single"
                 visibility="user" >
    
        <output>
            <content>
                <an:akomaNtoso>
                    <an:act>
                        <an:meta mode="call"/>
                        <an:preface mode="call" />
                        <an:preamble mode="call" />
                        <an:body  mode="call"/>
                        <an:conclusions mode="call" />
                    </an:act>
                </an:akomaNtoso>
            </content>
        </output>
    </sectionType>
	 
    <sectionType name="meta"
                 prefix="meta"
                 numstyle="single">
        <metadatas>
            <metadata name="BungeniExpAuthor" />
            <metadata name="BungeniExpAuthorURI" />
            <metadata name="BungeniPublicationSrcName" />
            <metadata name="BungeniPublicationDate" />
            <metadata name="BungeniPublicationDateHijri" />
            <metadata name="BungeniPublicationArea" />
            <metadata name="BungeniSourceType" />
            <metadata name="BungeniSourceNo" />
                
            <metadata name="BungeniActType" />
            <metadata name="BungeniActName" />
            <metadata name="BungeniActNo" />
            <metadata name="BungeniActYear" />
            <metadata name="BungeniActScope" />
            <metadata name="BungeniActState" />
            <metadata name="BungeniActFamily" />
            <metadata name="BungeniActHistoricalPeriod" />
            <metadata name="BungeniActEffectiveDate" />
            <metadata name="BungeniPageNo" />
            <metadata name="BungeniPageCount" />
            <metadata name="BungeniActFamily" />
            <metadata name="BungeniActFamilyPossible" />
            <metadata name="BungeniActCategory" />
 
            <metadata name="BungeniActRelease" />
            <metadata name="BungeniActReleaseDate" />
            <metadata name="BungeniActReleaseDateHijri" />
            <metadata name="BungeniActDeveloped" />
            <metadata name="BungeniActDevelopedDate" />
            <metadata name="BungeniActApproved" />
            <metadata name="BungeniActApprovedDate" />
        </metadatas>
            
        <output>
            <meta>
                <proprietary>
                    <muqid:AdditionalInfo>
                        <muqid:BungeniPublicationSrcName value="$BungeniPublicationSrcName" />
                        <muqid:BungeniPublicationDate value="$BungeniPublicationDate" />
                        <muqid:BungeniPublicationDateHijri value="$BungeniPublicationDateHijri" />
                        <muqid:BungeniPublicationArea value="$BungeniPublicationArea" />
                        <muqid:BungeniSourceType value="$BungeniSourceType" />
                        <muqid:BungeniSourceNo value="$BungeniSourceNo" />
                        
                        <muqid:BungeniActType value="$BungeniActType" />
                        <muqid:BungeniActName value="BungeniActName" />
                        <muqid:BungeniActNo value="$BungeniActNo" />
                        <muqid:BungeniActYear value="$BungeniActYear" />
                        <muqid:BungeniActScope value="$BungeniActScope" />
                        <muqid:BungeniActState value="$BungeniActState" />
                        <muqid:BungeniActFamily value="$BungeniActFamily" /> 
                        <muqid:BungeniActHistoricalPeriod value="$BungeniActHistoricalPeriod" />
                        <muqid:BungeniActEffectiveDate value="$BungeniActEffectiveDate" />
                        <muqid:BungeniPageNo value="$BungeniPageNo" /> 
                        <muqid:BungeniPageCount value="$BungeniPageCount" />
                        <muqid:BungeniActFamily value="$BungeniActFamily" />
                        <muqid:BungeniActFamilyPossible value="$BungeniActFamilyPossible" />
                        <muqid:BungeniActCategory value="$BungeniActCategory" />
                        
                        <muqid:BungeniActRelease value="$BungeniActRelease" /> 
                        <muqid:BungeniActReleaseDate value="$BungeniActReleaseDate" />
                        <muqid:BungeniActReleaseDateHijri value="$BungeniActReleaseDateHijri" />
                        <muqid:BungeniActDeveloped value="$BungeniActDeveloped" /> 
                        <muqid:BungeniActDevelopedDate value="$BungeniActDevelopedDate" />
                        <muqid:BungeniActApproved value="$BungeniActApproved" />
                        <muqid:BungeniActApprovedDate value="$BungeniActApprovedDate" />
                                   
                    </muqid:AdditionalInfo>
                </proprietary>      
            </meta>
            <content>
                  
                <an:meta />
            </content>
        </output>
    </sectionType>
   
