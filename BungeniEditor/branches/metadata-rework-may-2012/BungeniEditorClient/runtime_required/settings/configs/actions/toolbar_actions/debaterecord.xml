                    condition="textSelected:true :and:
                                cursorInSection:procmotion[0-9]*">
                    <title xml:lang="eng">····→ subheading</title>
                    <title xml:lang="spa">····→ subtitulo</title>
                    
                    <tooltip xml:lang="eng">
                        Personal Statement
                    </tooltip>
                </action>
            </blockAction>

            <blockAction name="Questions"
                
                target="null"
                visible="true" condition="none">
                <title xml:lang="eng">questions</title>
                <title xml:lang="spa">preguntas</title>
                
                <!-- create a narrative -->
                <action name="Question.narrative"
                    mode="TEXT_SELECTED_INSERT"
                    target="make_narrative_section"
                    visible="true"
                    condition="textSelected:true :and:
                                !cursorInSection:debaterecord :and:
                                !cursorInSection:narr[0-9]*">
                    <title xml:lang="eng">narrative</title>
                    <title xml:lang="spa">narrativa</title>
                    
                    <tooltip xml:lang="eng">
                        Mark selection as a Narrative
                    </tooltip>
                </action>
                <!--creates a question container -->
                <action name="QuestionsGroup.new"
                    mode="TEXT_SELECTED_INSERT"
                    target="make_debate_question_group_section"
                    visible="true"
                    condition="textSelected:true :and:
                                cursorInSection:debaterecord">
                    <title xml:lang="eng">questions section</title>
                    <title xml:lang="spa">sección de preguntas</title>
                    
                    <tooltip xml:lang="eng">
                        <html>
                        A group of many questions; this group of Questions <br />
                        are classifed on "Type"; this selector will prompt <br />
                        you for the "kind" of grouping.
                        </html>
                    </tooltip>
                </action>
                <action name="Questions.heading0"
                    mode="TEXT_SELECTED_INSERT"
                    target="apply_questiontext_style:heading_allquestions"
                    visible="true"
                    condition="textSelected:true :and:
                                cursorInSection:questions[0-9]*.">
                    <title xml:lang="eng">··→ heading</title>
                    <title xml:lang="spa">··→ titulo</title>
                    
                    <tooltip xml:lang="eng">
                        a heading for a group of Questions
                    </tooltip>
                </action>
                <!-- pops up a selector dialog for a questions - creates a questionanswer container -->
                <action name="questions.import.new"
                    mode="TEXT_SELECTED_INSERT"
                    target="make_debate_section_question"
                    visible="true"
                    condition="textSelected:true :and:
                                cursorInSection:questions[0-9]*">
                    <title xml:lang="eng">import question</title>
                    <title xml:lang="spa">importación pregunta</title>
                    
                    <tooltip xml:lang="eng">
                        <html>
                        A individual question is composed of a spoken question, <br />
                        speeches and one or more answers. This action will prompt <br />
                        you to select a question from a dialog screen and apply the <br />
                        chosen question's metadata on the selection.
                        </html>
                    </tooltip>
                </action>
                <action name="questions.add.new"
                    mode="TEXT_SELECTED_INSERT"
                    target="create_question_section"
                    visible="true"
                    condition="textSelected:true :and:
                                cursorInSection:questions[0-9]*">
                    <title xml:lang="eng">
                        add question
                    </title>
                    <title xml:lang="spa">
                        add question
                    </title>
                    
                    <tooltip xml:lang="eng">
                        <html>
                        A individual question is composed of a spoken question, <br />
                        speeches and one or more answers. This action will prompt <br />
                        you to enter the metadata for the question; and then apply <br />
                        the entered metadata on the selection.
                        </html>
                     </tooltip>
                </action>
                 <!-- markup text as question no. -->
                <action name="Questions.question_number"
                    mode="TEXT_SELECTED_INSERT"
                    target="markup_question_no"
                    visible="true"
                    condition="textSelected:true :and:
                                cursorInSection:qa[0-9]*">
                    <title xml:lang="eng">··→ number</title>
                    <title xml:lang="spa">··→ número de</title>
                    
                    <tooltip xml:lang="eng">
                        Identify the Question Number in the text
                    </tooltip>
                </action>
                <action name="Questions.heading1"
                    mode="TEXT_SELECTED_INSERT"
                    target="apply_questiontext_style:heading_question"
                    visible="true"
                    condition="textSelected:true :and:
                                cursorInSection:qa[0-9]*.">
                    <title xml:lang="eng">··→ heading</title>
                    <title xml:lang="spa">··→ titulo</title>
                    
                    <tooltip xml:lang="eng">
                        Heading for the spoken question
                    </tooltip>
                </action>
                <action name="Questions.subheading"
                    mode="TEXT_SELECTED_INSERT"
                    target="apply_questiontext_style:subheading_question"
                    visible="true"
                    condition="textSelected:true :and:
                                cursorInSection:qa[0-9]*.">
                    <title xml:lang="eng">····→ sub-heading</title>
                    <title xml:lang="spa">····→ sub-titulo</title>
                    
                    <tooltip xml:lang="eng">
                        Sub-Heading for the spoken question
                    </tooltip>
                </action>
                <action name="QuestionsSection.Q.new"
                    mode="TEXT_SELECTED_INSERT"
                    target="make_debate_section_section"
                    visible="true"
                    condition="textSelected:true :and:
                                cursorInSection:qa[0-9]* :and:
                                !sectionHasChild:sque[0-9]*">
                    <title xml:lang="eng">spoken question</title>
                    <title xml:lang="spa">pregunta oral</title>
                    
                    <tooltip xml:lang="eng">
                        <html>
                        A spoken question is the point where a member asks <br />
                        a question during the debate
                        </html>
                    </tooltip>
                </action>

                <!-- markup question bodyy-->
                <action name="Questions.body.markup"
                    mode="TEXT_SELECTED_INSERT"
                    target="make_question_body_section"
                    visible="true"
                    condition="textSelected:true :and:
                    cursorInSection:sque[0-9]*">
                    <title xml:lang="eng">··→ body</title>
                    <title xml:lang="spa">··→ cuerpo</title>
                    
                    <tooltip xml:lang="eng">
                        <html>
                        This is the body of the question. Usually the part of <br />
                        the spoken text that comes after the name of the person <br />
                        asking the question
                        </html>
                    </tooltip>
                </action>


                <!-- markup question by-->
                <action name="Questions.by.markup"
                    mode="TEXT_SELECTED_INSERT"
                    target="markup_question_by"
                    visible="true"
                    condition="textSelected:true :and:
                                cursorInSection:quebody[0-9]*">
                    <title xml:lang="eng">··→ by</title>
                    <title xml:lang="spa">··→ por</title>
                    
                    <tooltip xml:lang="eng">
                        Identify the name of the person asking the question.
                    </tooltip>
                </action>

                <action name="Questions.spoken_question.item_number"
                    mode="TEXT_SELECTED_INSERT"
                    target="markup_question_no"
                    visible="true"
                    condition="textSelected:true :and:
                    cursorInSection:quebody[0-9]*">
                    <title xml:lang="eng">··→ item num</title>
                    <title xml:lang="spa">··→ punto principale</title>
                    <tooltip xml:lang="eng">
                        Item Number
                    </tooltip>
                </action>

                <action name="Question.listitem"
                    mode="TEXT_SELECTED_INSERT"
                    target="make_speech_block_section:an-list-item"
                    visible="true"
                    condition="textSelected:true :and:
                                cursorInSection:quebody[0-9]*,sque[0-9]*,qa[0-9]*">
                    <title xml:lang="eng">··→ list-item</title>
                    <title xml:lang="spa">··→ list-item</title>
                    <tooltip xml:lang="eng">
                        Identify selected text as speech author's name
                    </tooltip>
                </action>

                <!-- markup quetion to
                <action name="Questions.to.markup"
                    mode="TEXT_SELECTED_INSERT"
                    target="toolbarSubAction.makeDebateQuestionBlockSection.markup_question_to"
                    visible="true"
                    condition="textSelected:true :and:
                                cursorInSection:sque[0-9]*">
                    <title xml:lang="eng">++++ to</title>
                    <tooltip xml:lang="eng">Question To </tooltip>
                </action>
                -->
            </blockAction>

            <blockAction name="Conclusion Markup"
                target="null"
                visible="true"
                condition="none">
                <title xml:lang="eng">adjournment</title>
                <title xml:lang="spa">suspensión</title>
                
                <action name="Conclusion.create"
                    mode="TEXT_SELECTED_INSERT"
                    target="make_adjournment_section"
                    visible="true"
                    condition="cursorInSection:debaterecord :and:
                                textSelected:true">
                    <title xml:lang="eng">adjournment section</title>
                    <title xml:lang="spa">suspensión section</title>
                    
                    <tooltip xml:lang="eng">Personal Statement </tooltip>
                </action>
                <action name="Conclusion.markHouseClosing"
                    mode="TEXT_SELECTED_INSERT"
                    target="mark_house_closing"
                    visible="true"
                    condition="cursorInSection:concl[0-9]* :and:
                                textSelected:true">
                    <title xml:lang="eng">··→ house adjournment time</title>
                    <title xml:lang="spa">··→ house suspensión time</title>
                    
                    <tooltip xml:lang="eng">Personal Statement </tooltip>
                </action>
            </blockAction>

        </actionGroup>
    </root>
</toolbar>
