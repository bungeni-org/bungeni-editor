<configs>
  <!--
  config/@url - can be either http://file.zip , file:/folder
  -->

  <config name="${config.name}"
    title="${config.title}"
    url="http://bungeni-editor.googlecode.com/svn/BungeniEditor_configs/${config.name}.zip"
    folder-base="${config.folder.home}${FILE_SEPARATOR}editor_configs"
    receiver="org.bungeni.editor.input.FSDocumentReceiver"
    default="true"
	/>
</configs>